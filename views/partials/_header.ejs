<head>
  <title>TinyApp</title>
  <a href= 'http://localhost:8080/urls'>List</a>
  <a href= 'http://localhost:8080/urls/new'>New</a>
  <a href= 'http://localhost:8080/register'>Register</a>
  <a href= 'http://localhost:8080/login'>Login</a>
  <% if(!username){ %>
  <form action="/login" method="POST" style="margin:10px;">
    <label for="username">Login</label>
    <input id="email" type="email" name="email" placeholder="email" style="width: 300px">
    <input id="password" type="password" name="password" placeholder="password" style="width: 300px">
    <input type="submit" value="Login">
  </form>
  <% } else { %>
  <form action="/logout" method="POST" style="margin:10px">
    <%= username %>
    <input type="submit" value="Logout">
  </form>
  <% } %>
</head>